<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - project</title>
   @*  <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/project.styles.css" asp-append-version="true" /> *@
    <!-- Favicon -->
    <link href="~/img/favicon.ico" rel="icon">

    <!-- Google Web Fonts -->
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Jost:wght@500;600;700&family=Open+Sans:wght@400;500&display=swap"
          rel="stylesheet">

    <!-- Icon Font Stylesheet -->
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.10.0/css/all.min.css" rel="stylesheet">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.4.1/font/bootstrap-icons.css" rel="stylesheet">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.5.1/css/all.min.css" integrity="sha512-DTOQO9RWCH3ppGqcWaEA1BIZOC6xxalwEsw9c2QQeAIftl+Vegovlnee1c9QX4TctnWMn13TZye+giMm8e2LwA==" crossorigin="anonymous" referrerpolicy="no-referrer" />
    <!-- Libraries Stylesheet -->
    <link href="~/lib/animate/animate.min.css" rel="stylesheet">
    <link href="~/lib/owlcarousel/assets/owl.carousel.min.css" rel="stylesheet">

    <!-- Customized Bootstrap Stylesheet -->
    <link href="~/css/bootstrap.min.css" rel="stylesheet">

    <!-- Template Stylesheet -->
    <link href="~/css/style.css" rel="stylesheet">
</head>
<body>
    <header>
        <!-- Spinner Start -->
        <div id="spinner"
             class="show bg-white position-fixed translate-middle w-100 vh-100 top-50 start-50 d-flex align-items-center justify-content-center">
            <div class="spinner-border text-primary" role="status" style="width: 3rem; height: 3rem;"></div>
        </div>
        <!-- Spinner End -->
        <!-- Navbar Start -->
        <div class="container-fluid fixed-top px-0 wow fadeIn" data-wow-delay="0.1s">
            <div class="top-bar row gx-0 align-items-center d-none d-lg-flex">
                <div class="col-lg-6 px-5 text-start">
                    <small><i class="fa fa-map-marker-alt text-primary me-2"></i>Al Barsha Dubai, UAE</small>
                    <small class="ms-4"><i class="fa fa-clock text-primary me-2"></i>24/7</small>
                </div>
                <div class="col-lg-6 px-5 text-end">
                    <small><i class="fa fa-envelope text-primary me-2"></i>info@StarSecurity.AE</small>
                    <small class="ms-4"><i class="fa fa-phone-alt text-primary me-2"></i>+971 02 667 5588</small>
                </div>
            </div>

            <nav class="navbar navbar-expand-lg navbar-light py-lg-0 px-lg-5 wow fadeIn" data-wow-delay="0.1s">
                <a href="index.html" class="navbar-brand ms-4 ms-lg-0">
                    <h1 class="display-5 text-primary m-0"><img src="~/img/logo.png" class="img img-fluid rounded-circle" width="110px" height="110px"></h1>
                </a>
                <button type="button" class="navbar-toggler me-4" data-bs-toggle="collapse"
                        data-bs-target="#navbarCollapse">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="collapse navbar-collapse" id="navbarCollapse">
                    <div class="navbar-nav ms-auto p-4 p-lg-0">
                        <a asp-area="" asp-controller="Home" asp-action="Index" class="nav-item nav-link active">Home</a>
                        <div class="nav-item dropdown">
                            <a  class="nav-link dropdown-toggle" data-bs-toggle="dropdown">Who We Are</a>
                            <div class="dropdown-menu border-light m-0">
                                <a asp-area="" asp-controller="About" asp-action="History" class="dropdown-item">History</a>
                                <a asp-area="" asp-controller="About" asp-action="BOD" class="dropdown-item">BOD</a>
                                <a asp-area="" asp-controller="About" asp-action="OurNetwork" class="dropdown-item">Our Network</a>
                            </div>
                        </div>
                        <div class="nav-item dropdown">
                            <a  class="nav-link dropdown-toggle" data-bs-toggle="dropdown">Services</a>
                            <div class="dropdown-menu border-light m-0">
                                <a asp-area="" asp-controller="Services" asp-action="ManGuard" class="dropdown-item">Man Guarding Service</a>
                                <a asp-area="" asp-controller="Services" asp-action="MobileSecurity" class="dropdown-item">Mobile Security Service</a>
                                <a asp-area="" asp-controller="Services" asp-action="LifeGuard" class="dropdown-item">Life Guard Service</a>
                                <a asp-area="" asp-controller="Services" asp-action="EventSecurity" class="dropdown-item">Event Security Service</a>
                                <a asp-area="" asp-controller="Services" asp-action="Bouncer" class="dropdown-item">Bouncer</a>
                                <a asp-area="" asp-controller="Services" asp-action="Residential" class="dropdown-item">Residential Security</a>
                                <a asp-area="" asp-controller="Services" asp-action="SMART" class="dropdown-item">Electronic Security System</a>
                            </div>
                        </div>
                        <a asp-area="" asp-controller="Home" asp-action="OurClient" class="nav-item nav-link">Our Clients</a>
                        <a asp-area="" asp-controller="Home" asp-action="Contact" class="nav-item nav-link">Contact</a>
                        <div class="nav-item dropdown">
                            <a  class="nav-link dropdown-toggle" data-bs-toggle="dropdown">Others</a>
                            <div class="dropdown-menu border-light m-0">
                                <a asp-area="" asp-controller="Others" asp-action="Vacancies" class="dropdown-item">Vacancies</a>
                                <a asp-area="" asp-controller="Others" asp-action="Hiring" class="dropdown-item">Hiring SOPs</a>
                            </div>
                        </div>
                    </div>
                    <div class="d-none d-lg-flex ms-2">
                        <a class="btn btn-light btn-sm-square rounded-circle ms-3" href="https://www.facebook.com/SSSDXB?mibextid=LQQJ4d">
                            <small class="fab fa-facebook-f text-primary"></small>
                        </a>
                        <a class="btn btn-light btn-sm-square rounded-circle ms-3" href="https://www.instagram.com/starsecurityservices_uae/?igshid=YmMyMTA2M2Y%3D">
                            <small class="fab fa-instagram text-primary"></small>
                        </a>
                        <a class="btn btn-light btn-sm-square rounded-circle ms-3" href="https://www.linkedin.com/company/star-security-services-llc">
                            <small class="fab fa-linkedin-in text-primary"></small>
                        </a>
                    </div>
                </div>
            </nav>
        </div>
        <!-- Navbar End -->
    </header>
    <div class="container-fluid">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>
    <!-- Footer Start -->
    <div class="container-fluid bg-dark text-light footer mt-5 py-5 wow fadeIn" data-wow-delay="0.1s">
        <div class="container py-5">
            <div class="row g-5">
                <div class="col-lg-3 col-md-6">
                    <img src="~/img/logo.png" class="img img-fluid">
                </div>
                <div class="col-lg-3 col-md-6">
                    <h4 class="text-white mb-4">Services</h4>
                    <a class="btn btn-link" asp-area="" asp-controller="Services" asp-action="ManGuard">Man Guarding Services</a>
                    <a class="btn btn-link" asp-area="" asp-controller="Services" asp-action="MobileSecurity">Mobile Security Services</a>
                    <a class="btn btn-link" asp-area="" asp-controller="Services" asp-action="LifeGuard">Life Guarding Services</a>
                    <a class="btn btn-link" asp-area="" asp-controller="Services" asp-action="EventSecurity">Event Security Services</a>
                    <a class="btn btn-link" asp-area="" asp-controller="Services" asp-action="Bouncer">Bouncer</a>
                    <a class="btn btn-link" asp-area="" asp-controller="Services" asp-action="Residential">Residential and Training</a>
                    <a class="btn btn-link" asp-area="" asp-controller="Services" asp-action="SMART">Electronic Security System</a>
                </div>
                <div class="col-lg-3 col-md-6">
                    <h4 class="text-white mb-4">Quick Links</h4>
                    <a class="btn btn-link" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                    <a class="btn btn-link" asp-area="" asp-controller="Home" asp-action="Contact">Contact Us</a>
                    <a class="btn btn-link" asp-area="" asp-controller="About" asp-action="History">About Us</a>
                    <a class="btn btn-link" asp-area="" asp-controller="" asp-action="">Clients</a>
                    <a class="btn btn-link" asp-area="" asp-controller="" asp-action="">Career With Us</a>
                </div>
                <div class="col-lg-3 col-md-6">
                    <h4 class="text-white mb-4">Our Office</h4>
                    <p class="mb-2"><i class="fa fa-map-marker-alt me-3"></i>Al Barsha Dubai, UAE</p>
                    <p class="mb-2"><i class="fa fa-phone-alt me-3"></i>+971 02 667 5588</p>
                    <p class="mb-2"><i class="fa fa-envelope me-3"></i>StarSecurity.AE</p>
                    <div class="d-flex pt-2">
                        <a class="btn btn-square btn-outline-light rounded-circle me-3" href="https://www.facebook.com/SSSDXB?mibextid=LQQJ4d">
                            <i class="fab fa-facebook-f"></i>
                        </a>
                        <a class="btn btn-square btn-outline-light rounded-circle me-3" href="https://www.instagram.com/starsecurityservices_uae/?igshid=YmMyMTA2M2Y%3D">
                            <i class="fab fa-instagram"></i>
                        </a>
                        <a class="btn btn-square btn-outline-light rounded-circle me-3" href="https://www.linkedin.com/company/star-security-services-llc">
                            <i class="fab fa-linkedin-in"></i>
                        </a>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- Footer End -->
    <!-- Copyright Start -->
    <div class="container-fluid copyright py-4">
        <div class="container">
            <div class="row">
                <div class="col-md-6 text-center text-md-start mb-3 mb-md-0">
                    &copy; <a class="border-bottom" href="#">StarSecurityServices_UAE</a>, All Right Reserved.
                </div>
                <div class="col-md-6 text-center text-md-end">

                    Group and Management By <a href="https://themewagon.com">L.L.C</a>
                </div>
            </div>
        </div>
    </div>
    <!-- Copyright End -->
    <!-- Back to Top -->
    <a href="#" class="btn btn-lg btn-primary btn-lg-square rounded-circle back-to-top">
        <i class="bi bi-arrow-up"></i>
    </a>


    <!-- JavaScript Libraries -->
    <script src="https://code.jquery.com/jquery-3.4.1.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/lib/wow/wow.min.js"></script>
    <script src="~/lib/easing/easing.min.js"></script>
    <script src="~/lib/waypoints/waypoints.min.js"></script>
    <script src="~/lib/owlcarousel/owl.carousel.min.js"></script>
    <script src="~/lib/counterup/counterup.min.js"></script>

    <!-- Template Javascript -->
    <script src="~/js/main.js"></script>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
